{
    "name": "zrone/service-governance-nacos",
    "description": "A nacos adapter for service governance.",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "service-governance",
        "nacos-adapter"
    ],
    "homepage": "https://hyperf.io",
    "support": {
        "docs": "https://hyperf.wiki",
        "issues": "https://github.com/hyperf/hyperf/issues",
        "pull-request": "https://github.com/hyperf/hyperf/pulls",
        "source": "https://github.com/hyperf/hyperf"
    },
    "require": {
        "php": ">=7.2",
        "hyperf/contract": "~2.2.0",
        "hyperf/nacos": "~2.2.0",
        "hyperf/service-governance": "~2.2.0",
        "hyperf/utils": "~2.2.0"
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\ServiceGovernanceNacos\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\ServiceGovernanceNacos\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev"
        },
        "hyperf": {
            "config": "Hyperf\\ServiceGovernanceNacos\\ConfigProvider"
        }
    }
}
